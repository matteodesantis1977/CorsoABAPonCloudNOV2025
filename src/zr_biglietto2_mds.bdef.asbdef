//  Progressivo;

  field ( readonly )
  CreatoDa,
  CreatoA,
  ModificatoDa,
  ModificatoA,
  IdBiglietto,
  Progressivo;

//  field ( readonly : update )
//  IdBiglietto,
//  Progressivo;

  update;
  delete;

  mapping for zcomponenti_mds corresponding extensible
    {
      IdBiglietto      = id_biglietto;
      Progressivo      = progressivo;
      TipoUtente       = tipo_utente;
      CreatoDa         = creatoda;
      CreatoA          = creatoa;
      ModificatoDa     = modificatoda;
